{
	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
		"rootDir": "./",
		"moduleResolution": "node10",
		"baseUrl": "./src",
		"paths": {
			"Src/*": [
				"./*"
			]
		},
		"allowJs": true,
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}